{
  "name": "adapteve-dapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && node ../../../scripts/required-env.js && vite build",
    "ci:build": "tsc && BUILD_ENV=test node ../../../scripts/required-env.js && vite build --outDir ../../../build_outputs/smart-assembly-base/dist",
    "lint": "prettier . --check && eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "prettier . --write \"**/*.{js,jsx,ts,tsx,css,scss,md}\" && eslint . --fix --report-unused-disable-directives --max-warnings 0",
    "deploy:devnet": "../../../scripts/deploy-dapp-to-environment.sh -n smart-assembly-base -e devnet",
    "preview": "nx preview",
    "test": "jest",
    "test:component": "cypress run-ct --headless",
    "test:headless": "synpress run --config supportFile='tests/support/index.ts',baseUrl=http://localhost:4173 --headless",
    "test:e2e": "synpress run --config supportFile='tests/support/index.ts',baseUrl=http://localhost:4173"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@eveworld/contexts": "0.0.8",
    "@eveworld/types": "0.0.8",
    "@eveworld/ui-components": "0.0.8",
    "@eveworld/utils": "0.0.8",
    "@mui/material": "^5.16.3",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "cypress": "^13.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-router-dom": "^6.25.0",
    "viem": "^2.17.4",
    "vite-plugin-svgr": "^3.3.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.7.0",
    "@synthetixio/synpress": "3.7.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "7.0.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.8",
    "globals": "^15.8.0",
    "postcss": "^8.4.39",
    "prettier": "3.3.2",
    "tailwindcss": "^3.4.5",
    "ts-jest": "^29.2.2",
    "typescript": "^5.5.3",
    "vite": "^5.4.6"
  },
  "nx": {
    "includedScripts": [
      "lint",
      "lint:fix",
      "ci:build",
      "deploy:devnet",
      "test:headless"
    ]
  }
}